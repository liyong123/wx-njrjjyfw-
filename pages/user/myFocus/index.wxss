.tabContent{
  display: flex;
  flex-direction: row;
  height: 80rpx;
  background-color:#fff;
  padding: 0 60rpx;
}
.tabContent view{
  display: flex;
  width: 50%;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color:#9b9b9b;
  position: relative
}
.tabContent view.choosedTab{
  color:#597ff3
}
.tabContent .bottomLine{
    position: absolute;
    bottom:8rpx;
    left:50%;
    width: 40rpx;
    height:4rpx;
    margin-left: -20rpx;
    background-color: #597FF3;
    border-radius: 2rpx;
}
.eachModule{
  display: flex;
  flex-direction: column;
  margin: 0 auto 20rpx;
  /* justify-content: center; */
}
.searchInput{
  padding:20rpx 0 0;
  margin:0 22rpx 0;
  position: relative;
  background-color: #fafafa
}
.searchInput icon{
  position: absolute;
  top:38rpx;
  left:32rpx;
}
.eachModule input{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 64rpx;
  width: 100%;
  border:solid 1px #c2cff6;
  border-radius: 32rpx;
  padding-left:84rpx;
  font-size:24rpx;
  color:#9b9b9b
}

.filter-list-header {
  display: flex;
  justify-content: space-between;
  height: 86rpx;
  border-bottom: 1rpx solid #E4E3E3;
  align-items: center;
}

.filter-list-header .list-header-item {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 25%;
}

.filter-list-header .list-header-item text {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #9B9B9B;
  letter-spacing: 0;
}

.filter-list-header .list-header-item .choosed {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #597FF3;
  letter-spacing: 0;
}

.filter-list-header .list-header-item image {
  width: 12rpx;
  height: 18rpx;
  margin-left: 10rpx;
}

.filter-list-header .list-header-item .filter-icon {
  width: 14rpx;
  height: 18rpx;
  margin-left: 10rpx;
}
.choosedCondition {
  display: flex;
  flex-direction: row;
  background-color:#eee;
  border-top:solid 2rpx #ddd ;
  border-bottom:solid 2rpx #ddd;
}
.choosedCondition view{
  height: 50rpx;
  width: 25%;
  border-right:solid 2rpx #ddd;
  display: flex;
  justify-content: center;
  align-items: center;
}
.choosedCondition view:last-child{
  border-right:none;
}
.eachModule .searchInput{
  display: flex;
  justify-content: center;
}
.company{
  display: flex;
  padding:26rpx 22rpx 0;
  flex-direction: column;
  border-radius:20rpx;
  margin:0 20rpx 20rpx;
  box-shadow:0rpx 4rpx 4rpx #dcdcdc;
  border:solid 1px #eee;
}




.companyTop{
  display: flex;
  flex-direction: row;
  margin-bottom:10rpx;
}
.companyTop view{
  margin-right:20rpx;
  display:flex;
}
.companyTop view image{
  width: 100rpx;
  height:100rpx;
  border-radius: 8rpx;
  background-color: #eee
}
.companyNameAdd{
  display: flex;
  flex-direction: column;
  position: relative;
  width:500rpx;
}
.companyTop  .nameAdressGrp{
  display: flex;
  flex-direction: row;
  margin-bottom:10rpx;
}
.companyTop .addressBrowse{
  display: flex;
  flex-direction: row;
  position: absolute;
  right: 0rpx;
  top:0rpx;
  margin-right:0
}
.companyTop .addressBrowse view text{
  font-size:20rpx;
}
.companyTop .nameOnly{
  width:350rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
  font-size:24rpx;
  color:#4a4a4a;
}
.addressBrowse view{
  display: flex;
  font-size:24rpx;
  align-items:center;
  flex-direction:row;
}
.addressBrowse view:first-child{
  color:#9b9b9b;
  font-size:24rpx;
  margin-right:0rpx;
}
.addressBrowse view:first-child image{
  display:flex;
  width:11px;
  height:13px;
  margin-right:8rpx;
}
.addressBrowse view:last-child text:last-child{
  color:#9b9b9b;
  font-size:20rpx;
}
.companyTop .registerTime{
   color:#666;
   font-size:24rpx;
   margin-bottom:10rpx;
}
.companyTop .moneyPersonGrp{
   color:#444;
   font-size:24rpx;
}
.companyBottom{
  padding:16rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: left;
  border-bottom:dotted 1px #eaeaea
}
.companyBottom view:first-child{
  width: 33%
}
.companyBottom view{
  display: flex;
  justify-content: center;
  align-items: center;
  height:40rpx;
  color:#9b9b9b;
  border-radius: 10rpx;
  width:30%;
  margin-right:1%;
  font-size:24rpx;
}
.companyBottom view text:last-child{
  color:#919191
}
.expertGroup{
  display: flex;
  flex-direction: column;
  margin: 0 20rpx 
}
.expertEach{
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  border:solid 1px #eee;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
  box-shadow: 0 4rpx 4rpx #dcdcdc
}
.expertPicture{
  display: flex;
  width: 50px;
  height: 50px;
  background-color: #dcdcdc;
  border-radius: 50px;
}
.expertPictureAll{
  display: flex;
  flex-direction: row;
  margin-right:20rpx;
  align-items: center;
}
.expertMessage{
  display: flex;
  flex-direction: column;
}
.expertMessage view{
  display: flex;
  flex-direction: row;
  margin-bottom:10rpx;
  align-items: center
}


/* .expertMessage view:first-child text:last-child{
  position: absolute;
  right: 0;
  top: 0
} */
.expertMessage .nameLevelGrp{
  position: relative;
  font-size:24rpx;
  display: flex;
  align-items: flex-start
}
.expertMessage .starAdressGrp{
   position: absolute;
   right:0;top:0
}




.nameLevelGrp text:first-child{
  font-weight: bold;
  color:#333;
  font-size:24rpx;
}
.nameLevelGrp text.abilitieLevel{
  color:#6E6E6E;
  font-size: 22rpx;
}
.workGroup{
  font-size: 24rpx;
  position: relative
}
.workGroup text{
  color:#9b9b9b;
}
.starGroup{
  margin-right:40rpx;
  position: relative
}
.starGroup image{
  width: 24rpx;
  height:24rpx
} 
.nameLevelGrp .starGroup text{
  color:#FF3A1B;
  font-size:16rpx;
  position:absolute;
  top:-8rpx;
  left:20rpx;
}
.workAddress image{
  display: flex;
  width: 22rpx;
  height:26rpx;
  margin-right: 8rpx;
}
.workGroup .workCompanyText{
  color:#777;
  width:350rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block
}





.domain-wrapper {
  display: flex;
  border-top: 1px dashed #EAEAEA;
  /* padding-top: 10rpx; */
  height: auto;
  align-items: flex-start;
  padding: 10rpx 0
}

.domain-wrapper .title {
  width: 140rpx;
  display: flex;
  justify-content: flex-start;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #636363;
  letter-spacing: 0;
  align-items: flex-start
}

.domain-wrapper .inner-content {
  width: 590rpx;
  display: flex;
  flex-wrap: wrap;
  font-size: 24rpx;
  color: #636363;
}
.domain-wrapper .inner-content text{
  color:#597FF3;
  background-color:#e8edfd;
  font-size: 24rpx;
  padding:2rpx 20rpx;
  border-radius: 20rpx;
  margin:0 10rpx 6rpx 0;
}
.loadingmore {
  color: #cccccc;
  width: 100%;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  margin: 30rpx 0;
}