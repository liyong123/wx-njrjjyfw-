.topBkg{
  display: flex;
  flex-direction: flex;
  height: 360rpx;
  background-color: #597FF3 ;
  justify-content: center;
}
.tabContent{
  display: flex;
  flex-direction: row;
  height: 56rpx;
  margin-top:60rpx;
  background-color:transparent;
  width: 90%;
}
.tabContent view{
  display: flex;
  width: 33%;
  justify-content: center;
  align-items: center;
}
.tabContent view text{
  width:188rpx;
  height:52rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color:#fff;
  border:solid 2rpx #fff;
  border-radius: 28rpx;
  /* opacity: 0.42; */

}
.tabContent view.choosedTab text{
  color:#2A57E8;
  /* opacity: 1.0; */
  background-color: #fff;
}
.mainContent{
  background-color: #fff;
  position: relative;
}
.totalDataContent{
  box-shadow:0rpx 10rpx 10rpx #dcdcdc;
  display: flex;
  flex-direction: column;
  width: 696rpx;
  height:360rpx;
  border-radius: 8rpx;
  position:absolute;
  top:-220rpx;
  left:50%;
  margin-left: -348rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,')
}
.totalArea{
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 180rpx;
  align-items: center;
  margin:0 52rpx ;
  border-bottom:dotted 2rpx #DFDFDF;
}
.totalImg > image{
  width: 160rpx;
  height:160rpx;
}
.totalArea .totalNum{
  display: flex;
  flex-direction: column;
  margin-left:20rpx;
}
.totalArea .totalNum text:first-child{
   font-size: 28rpx;
   color: #7A848E;
   font-weight:bold
}
.totalArea .totalNum text:last-child{
   margin-top:6rpx;
   font-size: 40rpx;
   color: #3B3FD9;
   font-weight:bold
}
.stateGrp{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height:180rpx;
}
.stateGrp>view{
  display: flex;
  flex-direction: column;
  width: 50%;
  padding-left:10%;
  justify-content: center;
}
.stateGrp view view:first-child{ 
  font-size: 26rpx;
  color: #94969E;
}
.stateGrp view view:last-child{ 
  font-size: 34rpx;
  color: #616B80;
  margin-top:10rpx;
}

.stateGrp view view text {
  font-size: 20rpx;
}

.data-board {
  padding-top: 160rpx;
  background-color: #fafafa;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.dash-line {
  width: 696rpx;
  height: 1rpx;
}

.datapart-wrap {
  box-sizing: border-box;
  box-shadow:0rpx 10rpx 10rpx #dcdcdc;
  border-radius: 6rpx;
  margin:20rpx 20rpx;
  width: 696rpx;

  padding: 0 30rpx;
}

.datapart-title {
  display:flex;
  padding: 30rpx 0 20rpx;
  align-items: center;
}

.datapart-title view {
  width: 4rpx;
  height: 26rpx;
  margin-right:10rpx;
  background-color:#597ff3; 
}

.dataprt-title text {
  color: #515558;
  font-size: 26rpx;
  line-height: 26rpx;
}

/*政府行业分布*/
.datapart-industry-item {
  display: flex;
  align-items: center;

  width: 100%;
  /* height: 60rpx; */

  box-sizing: border-box;
  /* padding: 0 20rpx; */

  border-bottom: 1rpx solid #e9ebee; 
}

.datapart-industry-item:last-child {
  border-bottom:none;
  margin-bottom: 20rpx;
}

.datapart-industry-item image {
  width: 20rpx;
  height: 20rpx;
  margin-right: 20rpx;
}

.datapart-industry-item text:nth-child(2) {
  color: #6e8cf4;
  font-size: 20rpx;
  margin-right: 20rpx;
  width: 42rpx;
  padding: 10rpx 0;
}

.datapart-industry-item text:nth-child(3) {
  color: #f5ae8b;
  font-size: 22rpx;
  margin-right: 20rpx;
  text-align: right;
  width: 60rpx; 
}

.datapart-industry-item text:nth-child(5) {
  flex: 1;
  font-size: 22rpx;
  text-align: right;
  width: 60rpx; 
  color: #6e8cf4;
}

.datapart-industry-item view {
  width: 380rpx;
  padding-right: 4rpx;
}

.datapart-industry-item view>view  {
  background-color: #aabefa;
  border-radius: 6rpx; 
  height: 6rpx;
}

.license-tab-placeholder{
  width:100%;
  height: 72rpx;
  border-top: 1rpx solid #e9ebee;
}

.license-tab-wrap {
  position: absolute;
  bottom: -1rpx;
  left: 0;
  right: 0;
  display: flex;
  justify-content: space-between;
 
  margin-top: 20rpx;
}

.license-tab-item {
  color: #b4b4b4;
  font-size: 26rpx;

  line-height: 70rpx;
  text-align: center;

  margin: 0 20rpx;

  flex: 1;
}

.license-tab-sel {
  color:#597ff3;
  border-bottom: 4rpx solid #597ff3;
}

.datapart-staff-item {
  display: flex;
  width: 100%;
  align-items: center;
  font-size: 20rpx;
  padding: 14rpx 0;
}

.datapart-staff-item:last-child {
  margin-bottom: 20rpx;
}

.datapart-staff-item>text{
  width:100rpx;
  text-align: right;
  color:#839ef2;
  margin-right: 20rpx;
}

.datapart-staff-item>view {
  flex: 1;
  background-color: #f6f8f9;
  height:26rpx;
  border-radius: 20rpx;

  display: flex;
  align-items: center;
}

.datapart-staff-item view view {
  background-color: #839ef2;
  text-align: right;
  padding-right: 16rpx;
  color: white;
  /* border-radius: 20rpx; */
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
}

.datapart-staff-item view text {
  background-color: transparent;
  text-align: left;
  padding-left: 16rpx;
  color: #f29a66;
}
