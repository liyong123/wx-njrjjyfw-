/* pages/web/demandDetail.wxss */

.wrapper {
  height: 100vh;
}

.fix-header {
  position: fixed;
  top: 0;
  left: 0;
  /* height: 174rpx; */
  height: auto;
  width: 100%;
  background-color: #fafafa;
  z-index: 1;
}

.list-search {
  display: flex;
  height: 64rpx;
  box-sizing: border-box;
  z-index: 20;
  justify-content: center;
  margin: 24rpx 20rpx 0 20rpx;
}

.search-title {
  flex-shrink: 0;
  font-size: 28rpx;
  padding-right: 10rpx;
}

.list-search-box {
  flex: 1;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  height: 64rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  border: 1rpx solid #c2cff6;
  border-radius: 32rpx;
}

.list-search-box input {
  width: 100%;
  height: 64rpx;
  padding-left: 10rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  /* color: #9B9B9B; */
  letter-spacing: 0;
}

.filter-container {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
}

.filter-icon {
  width: 28rpx;
  height: 36rpx;
  margin-left: 10rpx;
}

.filter-container text {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #9b9b9b;
  letter-spacing: 0;
}

.sort-list {
  display: flex;
  justify-content: space-between;
  height: 86rpx;
  border-bottom: 1rpx solid #e4e3e3;
  align-items: center;
}

.sort-list .sort-item {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 25%;
}

.sort-list .sort-item text {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #9b9b9b;
  letter-spacing: 0;
}

.sort-list .sort-item .choosed {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #597ff3;
  letter-spacing: 0;
}

.sort-list .sort-item image {
  width: 12rpx;
  height: 18rpx;
  margin-left: 10rpx;
}

.supplier-list-wrapper {
  margin-top: 194rpx;
  background: #fafafa;
}

.supplier-item-wrapper {
  border: 1rpx solid #eaeaea;
  border-radius: 15rpx;
  margin: 20rpx 20rpx 0 20rpx;
  background: white;
  box-shadow: 0 0 10rpx 10rpx #f0f5fa;
  padding: 0 20rpx;
  margin-bottom: 25rpx;
}

.supplier-item-wrapper:last-child {
  margin-bottom: 0;
}

.supplier-item-header {
  border-bottom: 2rpx dashed #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding: 15rpx 0 10rpx;
  position: relative;
}

.supplier-item-logo {
  width: 112rpx;
  height: 112rpx;
  border-radius: 56rpx;
  margin-right: 10rpx;
}

.supplier-item-detail {
  padding-top: 10rpx;
}

.supplier-item-name {
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  color: #353f49;
  letter-spacing: 0;
  font-weight: bold;
  width: 650rpx;
  overflow: hidden;
  display: inline-block;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.supplier-item-text {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #9b9b9b;
  letter-spacing: 0;
  line-height: 1.6;
}

.supplier-item-1781 {
  display: inline-block;
  font-size: 22rpx;
  color: #9b9b9b;
  letter-spacing: 0;
  line-height: 1.5;
  margin: 0 20rpx 0 180rpx;
}

.supplier-item-1781 text {
  float: right;
  font-family: PingFangSC-Light;
  font-size: 14rpx;
  color: #ff3a1b;
  margin-left: -10rpx;
}

.supplier-item-ocity {
  display: inline-block;
  font-size: 22rpx;
  color: #9b9b9b;
  letter-spacing: 0;
  line-height: 1.5;
}

.supplier-item-detail-right {
  position: absolute;
  top: 26rpx;
  right: 10rpx;
  display: flex;
  justify-content: flex-end;
}

.supplier-item-detail-right text {
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #9b9b9b;
  letter-spacing: 0;
  line-height: 1.5;
}

.supplier-item-detail-right-item {
  display: flex;
  align-items: center;
}

.supplier-item-detail-right image {
  width: 22rpx;
  height: 24rpx;
  margin-right: 6rpx;
}

.supplier_image {
  width: 22rpx;
  height: 24rpx;
  margin-right: 6rpx;
}

.supplier_image_1781 {
  width: 25rpx;
  height: 15rpx;
}

.loadingmore {
  color: #ccc;
  width: 100%;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  margin-top: 30rpx;
}

.no-order {
  text-align: center;
  margin-top: 228rpx;
}

.no-order .photo {
  width: 464rpx;
  height: 300rpx;
}

.no-order .tip {
  color: #4a4a4a;
}

.ad-image {
  height: 180rpx;
  width: 100%;
}

.ad-close {
  position: fixed;
  top: 20rpx;
  right: 20rpx;
  opacity: 0.6;
}
.swiperAll{
  height:180rpx;
  width:100%
}
.swiperAll image{
  width:100%;
  height:180rpx;
}
