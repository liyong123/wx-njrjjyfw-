/* pages/web/visitorIndex.wxss */

.content {
  width: 100%;
  overflow: hidden;
  margin-bottom: 60rpx;
}

.latest-title {
  width: 100%;
  height: 40rpx;
  border-left: 4rpx solid #597ff3;
}

.latest-title-text {
  display: inline-block;
  margin: 0 20rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.latest-title-image {
  width: 28rpx;
  height: 30rpx;
}

.view-more {
  float: right;
  font-size: 24rpx;
  color: #597ff3;
  margin-right: 25rpx;
  margin-top: 15rpx;
}

.view-more-1 {
  float: right;
  transform: scale(0.6, 2);
  -ms-transform: scale(0.6, 2);
  -webkit-transform: scale(0.6, 2);
  -moz-transform: scale(0.6, 2);
  -o-transform: scale(0.6, 2);
  margin-top: -2rpx;
}

/* 搜索供应商 */

.search {
  width: 100%;
  height: 100rpx;
  background-color: #597ff8;
  padding-top: 10rpx;
}

.search-content {
  width: 90%;
  height: 60rpx;
  background-color: #8ba6fa;
  margin: 0 auto;
  border-radius: 30rpx;
}

.search-image {
  float: left;
  width: 30rpx;
  height: 30rpx;
  margin: 14rpx 10rpx 0 35rpx;
}

.input-search {
  display: inline-block;
  width: 70%;
  height: 30rpx;
  margin: 5rpx;
  color: #fff;
}

.phcolor {
  color: #fff;
  opacity: 0.6;
}

/*  华为广告位*/

.advertising-post-background {
  width: 100%;
  height: 200rpx;
  /* border-bottom-left-radius: 300%;
  border-bottom-right-radius: 300%;
  background-color: #597ff3; */
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.advertising-post {
  width: 90%;
  height: 280rpx;
  margin: 0 auto;
  border-radius: 15rpx;
  margin-top: -200rpx;
  box-shadow: 0 0 10rpx 0 #545fdf;
  background-color: #fff;
}

.advertising-post image {
  width: 100%;
  height: 280rpx;
  border-radius: 15rpx;
}

/* 需求大厅  找供应商  项目热链接  杰英会 */

.itemize {
  width: 100%;
  padding: 15rpx 15rpx 25rpx 15rpx;
  border-bottom: 1rpx solid #dedede;
}

.itemize-classfiy {
  display: inline-block;
  width: 24%;
  margin: 0 auto;
  text-align: center;
}

.itemize-image {
  width: 122rpx;
  height: 122rpx;
}

/* 最新标讯 */

.burberry-glasses {
  width: 100%;
  height: 80rpx;
  margin: 15rpx 25rpx;
}

/* .burberry-glasses-image {
  width: 107rpx;
  height: 27rpx;
  margin-top: 35rpx;
} */

.burberry-black, .burberry-red {
  float: left;
  font-size: 28rpx;
  font-weight: bold;
  margin-top: 22rpx;
}

.burberry-black {
  color: #333;
}

.burberry-red {
  color: #e18273;
}

.burberry-glasses-content {
  float: right;
  width: calc(100% - 125rpx);
  height: 73rpx;
  padding-top: 13rpx;
  /* border-left: 1rpx solid #dedede; */
}

.burberry-glasses-newest {
  float: left;
  border: 1rpx solid #597ff3;
  color: #597ff3;
  padding: 5rpx;
  border-radius: 5rpx;
  font-size: 24rpx;
  margin: 10rpx 10rpx 0 10rpx;
  display: block;
}

.burberry-glasses-name {
  float: left;
  max-width: 350rpx;
  font-size: 26rpx;
  color: #4a4a4a;
  margin: 12rpx 10rpx 0;
  margin-right: 10rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.burberry-glasses-status {
  display: block;
  float: right;
  font-size: 26rpx;
  font-weight: bold;
  color: #e18273;
  margin: 12rpx 50rpx 0 0;
}

/* 最新需求 */

.latest-demand {
  padding-bottom: 30rpx;
  border-top: 20rpx solid #f8f8f8;
  border-bottom: 20rpx solid #f8f8f8;
  padding-top: 30rpx;
}

.demand-list {
  margin-top: 30rpx;
  border-top: 1rpx dashed #dedede;
}

.demand-list-con {
  width: 90%;
  margin: 0 auto;
  height: 140rpx;
  padding: 30rpx 0 0;
  border-bottom: 1rpx solid #dedede;
  font-size: 22rpx;
}

.demand-list-con-title {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: row;
}

.demand-list-con-title-sub {
  width: 450rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.demand-list-time, .demand-list-price {
  display: inline-block;
  /* font-size: 24rpx; */
}

.demand-list-time {
  width: 60%;
  border-right: 1rpx solid #dee5fe;
}

.demand-list-time>view {
  margin-bottom: 10rpx;
}

.demand-list-price {
  float: right;
  width: 180rpx;
  text-align: right;
  font-size: 28rpx;
}

.red {
  color: #e18273;
}

.demand-list-address {
  margin-top: 15rpx;
}

.demand-list-image {
  width: 25rpx;
  height: 25rpx;
  margin-right: 8rpx;
}

.icon_1776 {
  width: 25rpx;
  height: 25rpx;
  margin-right: 8rpx;
  margin-left: 30rpx;
}

.icon_1781 {
  width: 25rpx;
  height: 15rpx;
  margin-left: 30rpx;
  margin-right: 8rpx;
}

.icon1780 {
  width: 20rpx;
  height: 25rpx;
  margin-left: 30rpx;
  margin-right: 8rpx;
}

/* 最热门供应商 */

.hot-supplier {
  padding: 30rpx 0;
  margin-bottom: 100rpx;
}

.hot-supplier-con {
  margin-top: 30rpx;
  border-top: 1rpx dashed #dedede;
  padding-top: 30rpx;
}

.hot-supplier-list {
  display: inline-block;
  width: 170rpx;
  height: 170rpx;
  border: 1rpx solid #dedede;
  border-radius: 10rpx;
  margin-left: 10rpx;
  margin-top: 15rpx;
}

.hot-supplier-list>image {
  width: 140rpx;
  height: 140rpx;
  margin: 14rpx;
}

/* 登录云集  开启项目 */

.login-shade {
  width: 700rpx;
  height: 80rpx;
  background-color: #333;
  border-radius: 40rpx;
  opacity: 0.6;
  position: fixed;
  bottom: 140rpx;
  left: 10rpx;
}

.login {
  width: 700rpx;
  height: 80rpx;
  color: #fff;
  position: fixed;
  bottom: 140rpx;
  left: 10rpx;
}

.login-text {
  display: inline-block;
  font-size: 26rpx;
  margin: 20rpx 0 0 60rpx;
}

.login-operation {
  float: right;
  width: 225rpx;
  height: 60rpx;
  margin-top: 10rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #597ff3;
  border-radius: 30rpx;
  margin-right: 20rpx;
}

.publishGrp {
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: flex-start;
}
