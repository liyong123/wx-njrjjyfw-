.topBkg{
  display: flex;
  flex-direction: flex;
  height: 360rpx;
  background-color: #597FF3 ;
  justify-content: center;
}
.tabContent{
  height: 56rpx;
  margin-top:60rpx;
  background-color:transparent;
  width: 100%;
  padding: 0 42rpx;
  /* width: 90%; */
}
.tabContent view{
  display: inline-block;
  width:188rpx;
  height:52rpx;
  line-height: 52rpx;
  font-size: 24rpx;
  color:#fff;
  border:solid 2rpx #fff;
  border-radius: 28rpx;
  text-align: center;
  vertical-align: middle;
}
.tabContent view:nth-child(2){
  margin:0 40rpx
}
.tabContent view.choosedTab {
  color:#2A57E8;
  background-color: #fff;
}
.mainContent{
  background-color: #fff;
  position: absolute;
  top: 360rpx;
  width: 100%;
  bottom: 0;
}
.totalDataContent{
  display: flex;
  flex-direction: column;
  width: 696rpx;
  height:360rpx;
  border-radius: 8rpx;
  position:absolute;
  top:-220rpx;
  left:50%;
  margin-left: -348rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,')
}
.totalArea{
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 180rpx;
  align-items: center;
  margin:0 52rpx ;
  border-bottom:dotted 2rpx #DFDFDF;
}
.totalImg > image{
  width: 160rpx;
  height:160rpx;
}
.totalArea .totalNum{
  display: flex;
  flex-direction: column;
  margin-left:20rpx;
}
.totalArea .totalNum text:first-child{
   font-size: 28rpx;
   color: #7A848E;
   font-weight:bold
}
.totalArea .totalNum text:last-child{
   margin-top:6rpx;
   font-size: 40rpx;
   color: #3B3FD9;
   font-weight:bold
}
.stateGrp{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height:180rpx;
}
.stateGrp > view{
  display: flex;
  flex-direction: column;
  width: 50%;
  padding-left:10%;
  justify-content: center;
}
.stateGrp view > text:first-child{ 
  font-size: 26rpx;
  color: #94969E;
  font-weight: bold;
}
.stateGrp view > view:last-child{ 
  display: inline-block;
  font-size: 40rpx;
  color: #616B80;
  margin-top:10rpx;
  font-weight: bold;
}
.mapContent {
  padding-top:160rpx;
}
.mapContainer {
  margin: 0 auto;
  height: 700rpx;
  width: 600rpx;
  position: absolute;
  left: 150rpx;
  bottom: 0rpx;
  z-index: 999
}
.list-title {
  color: #979797;
  font-size: 26rpx;
  line-height: 40rpx;
  width: 100%;
  text-align: center;
  margin-bottom: 20rpx;
  vertical-align: middle;
  display: flex;
  justify-content: center;
}

.list-title > image {
  margin-right: 10rpx;
  margin-top: 2rpx;
}

.list-item {
  height: 58rpx;
  line-height: 58rpx;
  margin: 0 42rpx 0 42rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  border-bottom: 1px dashed #dedede;
  font-weight: bold;
  clear: both;
}

.list-item:nth-child(odd) {
  background-color: #f6f8fb;
}



.list-item:nth-child(2){
  border-top: 1px dashed #dedede;
}


.left {
  display: inline-block;
  color: #5a5a5a;
  font-weight: 600;
  float: left;
  line-height: 58rpx;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.right {
  display: inline-block;
  color: #666666;
  float: right;
  font-size: 22rpx;
  line-height: 58rpx;
}

.right.tuchu {
  color: #f5ad0e;
  font-size: 26rpx;
  margin-right: 10rpx;
}

.list-hot-icon {
  width: 30rpx;
  height: 35rpx;
  display: inline-block;
  float: left;
  margin: 12rpx 20rpx 0 0;
}

.list-point-icon {
  width: 8rpx;
  height: 8rpx;
  display: inline-block;
  float: left;
  margin: 25.5rpx 31rpx 13.5rpx 11rpx;
}

.scroll {
  position:absolute;
  height: 100%;
  width: 100%;
}


.totalArea2{
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 165rpx;
  align-items: center;
  margin-top:50rpx;
}

.stateGrp2{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height:auto;
  letter-spacing: 10rpx;
}
.stateGrp2 view text{
  color:#597FF3;
  font-size:40rpx;
  font-weight:bold
}
.stateGrp2 view text:last-child{
  color:#597FF3;
  font-size:26rpx;
  font-weight:normal
}

 .totalNum2 image{
   width:146rpx;
   height:57rpx;
}
 .totalNum2 text:first-child{
   width: 146rpx;
   height:57rpx;
   display: flex;
   font-size: 54rpx;
   color: #8D92BF;
   font-weight:bold
}
 .totalNum2 view:last-child{
   display: flex;
   flex-direction: row;
   margin-top:6rpx;
   font-size: 28rpx;
   color: #6E73A5;
   font-weight:bold
}
 .totalNum2 view:last-child view{
    width:4rpx;
    height:25rpx;
    background-color:#FFA000;
    margin-top:8rpx;
    margin-right:6rpx;
}

.one {
  top: -100%;
}

.two {
  top: 0%;
}

.three {
  top: 100%;
}

.two.top {
  top: -100%;
  transition: top .5s;
}

.two.bottom {
  top: 100%;
  transition: top .5s;
}

.one.bottom {
  top: 0%;
  transition: top .5s;
}

.three.top {
  top: 0%;
  transition: top .5s;
}

