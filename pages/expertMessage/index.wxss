page{
  display: flex;
  flex-direction: column;
  height: 100%;
  margin:0 auto;
}
.topHeaderBcg{
  height: 110rpx;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  background-color: #597ff3
}
.expert_header{
  background-color: #fff;
  height: auto;
  margin:0 20rpx;
  border-radius: 10rpx;
  box-shadow:0 16rpx 16rpx #e3e7f6;
  margin-top:-110rpx
}
.mainContent{
   margin-top: 0;
   margin-bottom: 0;
}
.picture_item{
  display: flex;
  justify-content:center;
  flex-direction: row;
  align-items:center;
  height: 190rpx;
  position:relative
}
.picture_item image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%; 
  background-color: #dcdcdc;
}
.nameGroup{
   display: flex;
   flex-direction: row;
   justify-content:center;
   position: relative
}
.nameGroup text:first-child{
  color: #4a4a4a;
  font-size: 36rpx;
  font-weight: 500;
}
.personalMessage{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin:14rpx 0 24rpx;
}
.personalIntrodution{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin:10rpx 0 0;
  height: 120rpx;
  align-items: center;
  border-top:dotted 1px #dedede;
  background-color:rgba(246, 248, 254, 0.8);
}
.introdutionEach {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.introdutionEach view:first-child{
  display: flex;
  width: 60rpx;
  height:60rpx;
  margin-right:26rpx;
}
.introdutionEach view:first-child image{
  width:60rpx;
  height: 60rpx;
  border-radius: 30rpx;
}
.introdutionEach view:nth-child(2){
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.introdutionEach view:nth-child(2) text:first-child{
  font-size: 30rpx;
  color:#597ff3;
  margin-bottom:8rpx;
}
.introdutionEach view:nth-child(2) text:nth-child(2){
  font-size: 22rpx;
  color:#7493bf;
}
.personalMessage text{
  color:#999;
  font-size:24rpx;
}
.scoreGroup{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-top:solid 1px #dcdcdc;
  height:110rpx;
  background-color: rgba(246, 248, 254, 0.8)
}
.scoreEach {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.scoreEach view:first-child{
  display: flex;
  width: 60rpx;
  height:60rpx;
  margin-right:26rpx;
}
.scoreEach view:first-child image{
  width:50rpx;
  height: 50rpx;
}
.scoreEach view:nth-child(2){
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.scoreEach view:nth-child(2) text:first-child{
  font-size: 24rpx;
  color:#597ff3;
  margin-bottom:8rpx;
}
.scoreEach view:nth-child(2) text:nth-child(2){
  font-size: 22rpx;
  color:#7493bf;
}
.scoreEachLine{
  display: flex;
  align-items: center;
  margin: 0 80rpx;
  width:1px;
  background-color:#597ff3;
  height:60%;

}
.addFocus{
  display: flex;
  flex-direction: row;
  justify-content: center;
  position: absolute;
  right:20rpx;
  top:34rpx;
}
.focusBtn{
  display: flex;
  flex-direction: row;
  justify-content: center;
  width: auto;
  padding: 0 10rpx;
  height: 40rpx;
  line-height: 38rpx;
  background-color: rgba(89, 127, 243, 0.9);
  border-radius: 8rpx;
  color: #fff;
  font-size: 24rpx;
  align-items: center;
}
.focusBtn image{
  width:24rpx;
  height:24rpx;
  margin-right:6rpx;
  border-radius: 0;
  background-color: transparent
}
.focusBtn2{
   background-color:#dcdcdc
}
.focusBtn2{
  border:solid 1px #dcdcdc;
}
.eachModule{
  display: flex;
  flex-direction: column;
  padding: 20rpx 0;
  background-color: #fff;
  margin:20rpx 0 0 0;
  box-shadow:0 16rpx 16rpx #e3e7f6;
  border-radius: 8rpx;
}
.identitySelf{
  display: flex;
  flex-direction: row;
  padding: 0 10px;
  margin-bottom: 20rpx;
}
.circle{
  display: flex;
  justify-content: center;
  margin-top:12rpx;
  margin-right:20rpx;
}
.circle text{
   width:10rpx;
   height:10rpx;
   border-radius: 10rpx;
   background-color:#597ff3
}
.identityTitle{
  display:flex;
  align-items: flex-start;
  width: 50px;
  font-size:24rpx;
  color:#9b9b9b;
}
.identityContent{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 24rpx;
  color:#666;
}
.identityEach{
  height: auto;
  align-items: center;
  border-radius: 10rpx;
  color: #597ff3;
  font-size:24rpx;
  padding: 4rpx 10rpx;
  margin-right:10rpx;
  margin-bottom:10rpx;
  background-color: rgba(227, 232, 254, 0.8)
}
.showDown{
  height: auto;
  color: rgba(89, 127, 243, 0.8);
  font-size:22rpx;
  padding: 6rpx 10rpx 0
}
.showDown image{
  width: 20rpx;
  height:12rpx;
  margin-left:10rpx;
}
.eachModuleTitle{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-top:10rpx;
}
.illustration{
  display: flex;
  color:#9b9b9b;
  font-size: 24rpx;
  font-weight: bold;
  margin-top:20rpx;
  padding:20rpx 0 20rpx 20rpx;
  border-top:dotted 1px #dedede
}
.titleBckground{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height:52rpx;
  background-color: rgba(89, 127, 243, 0.1);
  border-top-right-radius: 24rpx;
  border-bottom-right-radius: 24rpx;
}
.titleBckground view{
  display: flex;
  flex-direction: row;
  align-items: center
}
.titleBckground view text:first-child{
  display: flex;
  width: 6rpx;
  height:30rpx;
  background-color: #597ff3;
  margin-right:20rpx;
}
.titleBckground view text:nth-child(2){
  display: flex;
  margin-right:20rpx;
  font-size: 30rpx;
  color:#587ef2
}
.titleBckground view  image{
   display: flex;
   width:24rpx;
   height:28rpx;
   margin-right:20rpx;
}
.eachExperience{
  display: flex;
  flex-direction: row;
  padding: 20rpx 0;
  margin: 0 20rpx;
  border-bottom:dotted 1px #aaa
}
.eachExperience2{
  border-bottom:none;
}
.eachModule .eachExperience:last-child{
   border-bottom:none
}
.eachExperienceLeft{
  display: flex;
  flex-direction: column;
  position: relative
}
.expericenceCircle{
  width:12rpx;
  height:12rpx;
  background-color:#4876FF;
  border-radius:12rpx;
  margin-top:12rpx;

}
.expericenceLine{
  position: absolute;
  top: 20rpx;
  left:10rpx;
  bottom: 0;
  width: 4rpx;
  height: 106rpx;
  z-index: 990;
  border-left: solid 1px #4876FF;
}
.expericenceContent{
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
}
.expericenceTime{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin-bottom: 20rpx;
  font-size:24rpx;
  color:#597ff3
}
.workDuty{
  display: flex;
  flex-direction: row;
}
.workDuty text:first-child{
  color: #666;
  font-size:24rpx;
}
.workDuty text:last-child{
  color: #999;
  font-size:24rpx;
}
.seeWorkExpericenceAll{
   display: flex;
   justify-content: center;
   position:absolute;
   width: 100%;
   bottom:20rpx;
}
.seeWorkExpericenceAll view{
   padding:8rpx 20rpx;
   border:solid 1px #597ff3;
   border-radius: 40rpx;
   width:230rpx;
   font-size:24rpx;
   display:flex;
   align-items:center;
}
.seeWorkExpericenceAll text{
   color:#597ff3; 
}
.seeWorkExpericenceAll image{
  width:20rpx;
  height:12rpx;
  margin-left:6rpx;
}
.eachModule .eachExperience:last-child .expericenceLine{
  height: 80rpx;
}
.workContent{
  display: flex;
  padding:20rpx;
  font-size:24rpx;
  flex-direction: column
}
.workContent2{
  background-color: rgba(246, 248, 254, 0.8);
  padding:30rpx 20rpx;
  border-top:solid 1px #dcdcdc
}
.workResultTitle{
  color:#999;
  font-size:24rpx;
  margin-bottom:20rpx;
}
.workResultContent{
  display: flex;
  font-size: 24rpx;
}
.joinContent{
  display: flex;
  flex-direction: row;
  font-size: 24rpx;
  color:#666;
  margin-left:20rpx;
  padding-bottom:20rpx;
  margin-bottom:20rpx;
  border-bottom: dotted 1px #ccc;
  align-items: center
}
.joinContent text:first-child{
  display: flex;
  justify-content: center;
  align-items: center;
  width:22rpx;
  height:22rpx;
  border:solid 1px #747474;
  border-radius: 22rpx;
  font-size:18rpx;
  color:#666;
  margin-right:16rpx;
}
.joinContent text:last-child{
  font-size: 24rpx;
  color:#666;
}
.contentFooter{
  position: fixed;
  background-color: #dcdcdc;
  height: 100rpx;
  bottom:0;
  left: 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center
}
.contentFooter view{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
}
.contentFooter view:first-child{
  width: 20%
}
.contentFooter view:nth-child(2){
  width: 20%
}
.contentFooter view:last-child{
  width: 60%;
  background-color:#66cdaa
}
.introdutionPersonal{
  display: flex;
  justify-content: center;
  margin: 0 0 20rpx 0;
  color:rgba(89, 127, 243, 0.8);
  font-size:22rpx;
  align-items: center
}
.introdutionPersonal image{
  width: 20rpx;
  height:12rpx;
  margin-left:6rpx;
}
.formItem{
  display: flex;
  flex-direction: row;
  margin-bottom:16rpx ;
  height:100rpx;
  align-items: center;
  background-color: rgba(246, 248, 254, 0.6)
}
.formItem view:first-child{
  display: flex;
  justify-content: center;
  align-items: center;
}
.formItem view:first-child text:first-child{
  display: flex;
  margin-right:10rpx;
  color:#f12424
}
.formItem view:first-child text:last-child{
  font-size:28rpx;
  color:#999;
  display: flex
}
.formItem view:last-child input{
  width:95%;
  color:#999
}
.formItem2{
  display: flex;
  flex-direction: column;
  align-items: flex-start
}
.formItem view:last-child{
  display: flex;
}
.formItem view:last-child textarea{
  display: flex;
  margin:0 auto;
  color:#999;
  min-height: 300rpx;
}
.submitBtnArea{
  margin: 20rpx ;
}
.submitBtnArea button{
  background-color: #597ff3;
  height:100rpx;
  line-height: 100rpx;
  color:#fff;
  font-size:32rpx;
  border:none;
  border-radius: 16rpx
}