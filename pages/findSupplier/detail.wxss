.bast image {
  display: block;
  width: 38rpx;
  height: 38rpx;
  /* margin-left: 31rpx; */
  margin-top: 10rpx;
  margin-bottom: 8rpx;
}

.share-button {
  width:100%;
  height:100rpx;
  position:absolute;
  top:0;
  left:0;
  opacity:0;
}

.not-followed {
  background-color: #4f7efb;
  color: white;
}

.followed {
  background-color: #d9d9d9;
  color:#fdfafa;
}

/* 右侧返回顶部  快捷导航 */

.back-top {
  position: fixed;
  bottom: 180rpx;
  right: 40rpx;
}

.back-image {
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx 0 0 20rpx;
}

.back-top>text {
  display: inline-block;
  width: 99rpx;
  text-align: center;
  position: absolute;
  bottom: 10rpx;
  right: 3rpx;
}

.quick-navigation {
  position: fixed;
  bottom: 300rpx;
  right: 40rpx;
}

.quick-navigation-detail {
  width: 79rpx;
  border-radius: 50rpx;
  border: 1rpx solid #4f7efb;
  border-bottom: 0;
  padding: 15rpx 0;
  padding-bottom: 70rpx;
  color: #4f7efb;
  background-color: #fff;
  opacity: 0.8;
  position: fixed;
  bottom: 317rpx;
  right: 50rpx;
}

.quick-navigation-detail>text {
  display: block;
  width: 55rpx;
  margin: 15rpx 14rpx;
  text-align: center;
  border-bottom: 1rpx solid #4f7efb;
  padding-bottom: 15rpx;
}

.quick-navigation-detail>text:nth-last-child(1) {
  border-bottom: 0;
}

/* 资质证书  图片 */

.mask-layer {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
  left: 0;
}

.mask-bottom-content {
  background: white;
  position:fixed;
  bottom: 0;
  left: 0;
}

.mask-cancle {
  font-size:34rpx;
  width:100%;
  text-align:center;
  line-height:100rpx;
  height:100rpx;
}

.mask-layer-hide {
  color: #ffffff;
  font-size: 55rpx;
  position: fixed;
  top: 44rpx;
  right: 44rpx;
}

.certificate-image {
  width: 90%;
  height: 60%;
  position: fixed;
  top: 20%;
  left: 5%;
}

.certificate-image>image {
  width: 100%;
  height: 100%;
}

.have-no-data {
  padding: 20rpx 20rpx 30rpx;
}

.detail-scroll {
  height: 500rpx;
  border-top:1rpx solid #dedede;
  border-bottom:1rpx solid #dedede;
}