/* pages/web/detail.wxss */
.detail_nodata{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;

  font-size: 24rpx;
  color:#9B9B9B;

  width: 100%;
  padding-top: 40%;
}

.detail_nodata image{
  width:400rpx;
  height:341rpx;
  margin-bottom:20rpx;
} 

.content {
  font-size: 24rpx;
  background-color: #f8f8f8;
  padding-bottom: 106rpx;
}

/* 查看全部 */
.look-at-all-container {
  padding-bottom:1rpx;
  display: flex;
}
.look-at-all-classfy {
  height: 55rpx;
  line-height: 55rpx;
  color: #4f7efb;
  border: 1rpx solid #4f7efb;
  border-radius: 27rpx;
  text-align: center;
  margin: 30rpx auto;
  margin-top: 30rpx;
  padding: 0 30rpx;
}

.look-at-all-classfy>image {
  width: 30rpx;
  height: 17rpx;
  margin-left: 15rpx;
}


/* 基本信息 */
.basic-bk {
  width: 100%;
  height: 600rpx;

  position:relative;
}

.maturity_analysis{
  position: absolute;
  top: 4rpx;
  right: 25rpx;
  width: 208rpx;
  height: 50rpx;
  z-index: 999;
}
.basic-bk-image0 {
  width: 100%;
  height: 370rpx;

  position: absolute;
  top:-170rpx;
  left:0;
}

.basic-bk-image1 {
  width: calc(100% - 20rpx);
  height: 600rpx;

  position: absolute;
  top: -20rpx;
  left: 22rpx; /*左右阴影不一样宽。。。。坑*/
  z-index: 1;
}

.basic-container {
  display: flex;
  flex-direction: column;
  align-items: center;

  position: absolute; 

  z-index: 2;
  top: 30rpx;
  left: 40rpx;
  right: 40rpx;
}

.basic-logo {
  width:120rpx;
  height: 120rpx;
  border-radius: 10rpx;
  margin: 20rpx;
  background: white;
}

.company-name {
  font-size: 32rpx;
  color: #4a4a4a;
  font-weight: bold;
  line-height: 58rpx;
  margin-bottom: 10rpx;
}

.people-no {
  color: #f64e4e;
}

.gray-color {
  color: #b0b0b0;
}

.white-color {
  color: white;
}

.flex-container {
  display: flex;
  align-items: center;
  justify-content: center;
}

.image-address {
  width: 16rpx;
  height: 20rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.blue-color {
  color:#597ff3;
}

.blue-color2 {
  color:#9bacf6;
}

.dash-line {
  width: 100%;
  border-bottom: 1rpx dashed #cfcccc;
}

.basic-classify-contianer {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width:96%;
}

.basic-classify {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;

  padding: 30rpx 0;
}

.basic-classify image {
  width:53rpx;
  height:53rpx;
  margin-right: 10rpx;
}

.basic-classify-text-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.basic-classify-contianer2 {
  justify-content: space-around;
  margin: 30rpx;
  padding-bottom: 20rpx;
}

.order-number {
  line-height: 45rpx;
  font-size: 34rpx;
}

.order-number-mark {
  font-size: 24rpx;
}

/*各部分通用*/
.part-container {
  background: white;
  margin-bottom: 20rpx;
}
.part-header {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-bottom:2rpx dashed #b6b4b4;
}
.part-header-left {
  background:#597ff3;
  width:4rpx;
  height:28rpx;
}
.part-header-title {
  font-size: 28rpx;
  margin: 0 10rpx;
}

/* 公司简介 */
.cpmpany-brief-icon {
  width:26rpx;
  height:28rpx;
}

.cpmpany-brief-1 {
  padding: 20rpx 25rpx;
  line-height: 40rpx;
  color: #666;
  padding-bottom: 25rpx;
}

.look-detail {
  display: inline-block;
  color: #4f7efb;
  margin-left: 30rpx;
}

/* 擅长的领域 */

.field-expertise-icon {
  width:26rpx;
  height:28rpx; 
}

.field-expertise-1 {
  padding: 20rpx 0;
  background-color: #fff;
}

.field-expertise-1>text {
  display: inline-block;
  background-color: #e8eefd;
  height: 35rpx;
  line-height: 35rpx;
  border-radius: 10rpx;
  color: #4f7efb;
  padding: 0 12rpx;
  margin: 15rpx;
}

/* 成功案例 */
.success-case-icon {
  width: 28rpx;
  height: 28rpx;
}

.case-header {
  display: flex;
  justify-content: space-between;
  justify-items: center;
  margin-bottom: 15rpx;
}

.success-case {
  padding: 25rpx 0;
  margin: 0 25rpx;
  border-bottom: 1rpx solid #ebebeb;
}

.success-case:nth-last-child(1) {
  border: none;
}

.case-name {
  font-weight: bold;
  line-height: 45rpx;
}

.case-classfy>view:nth-child(2n-1) {
  display: inline-block;
  width: 60%;
  margin: 5rpx 0;
  line-height: 30rpx;
}
.case-classfy>view:nth-child(2n) {
  display: inline-block;
  width: 30%;
  margin: 5rpx 0;
  line-height: 30rpx;
}

.case-desc {
  border-top: 1rpx solid #dedede;
  border-bottom: 1rpx solid #dedede;
  padding: 30rpx 20rpx;
  word-break:break-word;
}

/* 解决方案 */
.solution-icon {
  width: 24rpx;
  height: 28rpx;
}

.solution {
  display: inline-block;
  width: 46%;
  margin: 10rpx 0;
  margin-left: 2%;
  margin-right:2%;
}

.solutions {
  border-bottom: 1rpx solid #ebebeb;
  margin: 15rpx 2% 0;
}

.solution>image {
  display: inline-block;
  width: 100%;
  height: 255rpx;
}

.solution-name {
  line-height: 32rpx;
  margin: 15rpx 0;
}

.solution-price {
  line-height: 32rpx;
  color: #f48942;
  font-size: 20rpx;
}

.solution-price>text {
  font-size: 32rpx;
}

/* 核心成员 */
.core-member-icon {
  width: 26rpx;
  height: 26rpx;
}

.core-member {
  margin: 30rpx 25rpx 0;
  display: flex;
  border-bottom: 1rpx solid #ebebeb;
  padding-bottom: 20rpx;
}

.core-member image {
  height: 142rpx;
  width: 142rpx;
  border-radius: 50%;
}

.member-detail {
  margin-left: 10rpx;
  border-left: 1rpx solid #dedede;
  padding-left: 30rpx;
}

.member-name {
  font-size: 26rpx;
  font-weight: bold;
  text-align: center;
}

.member-detail-item {
  display: flex;
  margin-top:10rpx;
}

.member-detail-label {
  min-width:150rpx;
  margin-top:10rpx;
}
.member-detail-vals {
  display: flex;
  flex-wrap: wrap;
}

.member-detail-val {
  background-color: #e8eefd;
  height: 35rpx;
  line-height: 35rpx;
  border-radius: 10rpx;
  color: #4f7efb;
  padding: 0 10rpx;
  margin: 10rpx;

  white-space: nowrap;
}

.core-member:nth-last-child(1) {
  border: none;
}

/* 资质证书 */
.certificate {
  line-height: 65rpx;
  margin: 15rpx 25rpx;
  border-bottom: 1rpx solid #dedede;
}

.certificate>image {
  float: right;
  width: 50rpx;
  height: 50rpx;
}

.certificate:nth-last-child(1) {
  border:none;
}

/* 风险信息 */
.risk-information {
  align-items: center;
  text-align: center;
  padding-bottom:20rpx;
}

.risk-information>image {
  display: block;
  width: 290rpx;
  height: 190rpx;
  margin: 0 auto;
  margin-top: 25rpx;
}

.risk-information>view {
  width: 100%;
}

/*  底部固定*/

.bast {
  width: 100vw;
  height: 106rpx;
  position: fixed;
  bottom: -3rpx;
  left: 0;

  display: flex;
  justify-content: center;
}

.bast-item-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-top: 1rpx solid #dedede;
  flex-grow: 1;
  background-color: #fff;
}

