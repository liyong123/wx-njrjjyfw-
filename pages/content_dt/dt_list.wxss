.content-dt{
  width:95%;
  margin:0 auto;
}
.dt_tu{
  display:flex;
  justify-content:flex_end;
  align-items:center;
  flex-direction:column;
  margin:150rpx auto;
}
.dt_tu image{
  width:300rpx;
  height:300rpx;
  margin:0 auto;
}
.dt_wen{
  margin-top:50rpx;
  font-size:32rpx;
  color:#9B9B9B;
  text-align:center;
}
.dt-time{
  height:90rpx;
  margin:20rpx auto;
  background:#fff;
  display:flex;
  align-items:center;
  justify-content: flex-end;
  padding-right:30rpx;
}
.dt-times{
  float:left;
  font-size:30rpx;
}
.dt-times-img{
  display:inline-block;
  margin-left:20rpx;
}
.dt-times-img image{
  width:40rpx;
  height:40rpx;
}
.dt-list-ys{
  padding:10rpx;
