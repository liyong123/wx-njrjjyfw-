  margin:20rpx auto;
}
.dt-ys-1{
  height:100rpx;
  line-height: 100rpx;
  border-bottom:1px solid #ccc;
  display:flex;
  justify-content: flex-start;
  align-items:center;
}
.dt-ys-1 image{
  width:50rpx;  
  height:50rpx;
  margin:10rpx 10rpx;
}
.dt-ys-1 text{
  margin-right:10rpx;
}
.dt-ys-2{
  display:flex;
  justify-content: space-between;
}

.dt_des{
  padding:20rpx;
  width:100%;
}

.dt_des text{
  font-size:24rpx;
  display:inline-block;
}
.dt-ys-3{
  padding:30rpx 20rpx;
}
.dt-ys-3 image{
  width:50rpx;
  height:40rpx;
}
.dt_des image{
  width:50rpx;
  height:50rpx;
  float:left;
}
.meeting{
  float:left;
  font-size:24rpx;
  margin-left:20rpx;
}
