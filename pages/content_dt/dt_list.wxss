  display:flex;
  flex:1;
  flex-direction:column;
  align-items:center;
  font-size:23rpx;
}
.commun2_223line_text{ width: 100%;}
.commun2_223line{ width: 100%; height: 4rpx; background: #0AC18A; border-radius: 2rpx;}
